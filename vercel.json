{
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "api/(.*)",
        "dest": "api/main.py"
      }
    ],
    "excludeFiles": [
      "**/*.pyc",
      "**/__pycache__/**",
      ".env",
      ".git/**",
      ".venv/**",
      "automations/**",
      "docs/**",
      "logdir/**",
      "tests/**",
      "prisma/migrations/**",
      "Dockerfile",
      "README.md",
      "pytest.ini",
      "package.json"
    ]
  }